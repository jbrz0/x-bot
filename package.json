  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/scheduler.js",
    "dev": "nodemon --watch src --ext ts --exec 'ts-node src/scheduler.ts'",
    "run-now": "ts-node src/bot.ts",
